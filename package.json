{
  "name": "alphagamebot",
  "type": "module",
  "version": "4.0.0",
  "description": "A Discord bot that's free and (hopefully) doesn't suck.",
  "main": "index.js",
  "scripts": {
    "build:clean": "rm -rf ./dist && tsc --build --verbose",
    "test": "export NODE_ENV=test && node --loader ts-node/esm --no-warnings=ExperimentalWarning --experimental-vm-modules node_modules/jest/bin/jest.js src/ --forceExit --silent",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "build:start": "npm run build:clean && echo ---starting--- && node ./dist/main.js",
    "build:deploy": "npm run build:clean && node ./dist/deploy-commands.js",
    "build": "tsc --build --verbose",
    "start": "node ./dist/main.js",
    "deploy": "node ./dist/deploy-commands.js"
  },
  "keywords": [
    "Discord",
    "Engagement"
  ],
  "author": "Damien Boisvert",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "@prisma/client": "^6.17.1",
    "discord.js": "^14.23.1",
    "pope": "^3.0.0",
    "prom-client": "^15.1.3",
    "winston": "^3.18.3",
    "winston-loki": "^6.1.3",
    "winston-transport": "^4.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-typescript": "^7.27.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.7.0",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "babel-jest": "^30.2.0",
    "dotenv": "^17.2.3",
    "jest": "^30.2.0",
    "jiti": "^2.6.1",
    "prisma": "^6.17.1",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.0"
  }
}
